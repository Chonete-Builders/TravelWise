{
    "version": 2,
    "builds": [
      {
        "src": "travel-wise-web/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "travel-wise-web/build/$1"
      }
    ]
  }